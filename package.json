{
  "name": "formidable-landers",
  "version": "8.4.0",
  "description": "Reusable components for Formidable's marketing sites",
  "main": "lib/index.js",
  "scripts": {
    "demo": "webpack-dashboard -- webpack-dev-server --port 5678 --config demo/webpack.config.demo.js --colors --content-base demo",
    "lint": "eslint src test",
    "test": "karma start karma.conf.js",
    "check": "yarn run lint && yarn run test",
    "babel": "rimraf lib && babel src -d lib",
    "copy-assets": "rimraf lib/assets && cp -r src/assets lib/assets",
    "copy-styles": "rimraf lib/styles && postcss 'src/styles/*.css' --verbose --dir lib/styles",
    "build": "yarn run babel && yarn run copy-assets && yarn run copy-styles",
    "preversion": "yarn run check",
    "version": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/formidable-landers.git"
  },
  "author": "david.davidson@formidable.com, paula.lavalle@formidable.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/formidable-landers/issues"
  },
  "homepage": "https://github.com/FormidableLabs/formidable-landers#readme",
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^16.8.4",
    "react-side-effect": "^1.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme": "^3.3.0",
    "eslint-config-formidable": "^3.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint": "^4.15.0",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.2.0",
    "json-loader": "^0.5.7",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "karma": "^2.0.2",
    "mocha": "^5.1.1",
    "normalize.css": "^8.0.0",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-inline-svg": "^3.1.1",
    "postcss-loader": "^2.1.4",
    "postcss-url": "^7.3.2",
    "prettier": "^1.12.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.1.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.4",
    "webpack": "^4.7.0"
  }
}
