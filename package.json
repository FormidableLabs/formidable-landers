{
  "name": "formidable-landers",
  "version": "6.0.1",
  "description": "Reusable components for Formidable's marketing sites",
  "main": "lib/index.js",
  "scripts": {
    "demo": "webpack-dashboard -- webpack-dev-server --port 5678 --config demo/webpack.config.demo.js --colors --content-base demo",
    "lint": "eslint --ext .js,.jsx src",
    "test": "karma start ./karma.conf.js",
    "test-travis": "karma start ./karma.conf.js --singleRun=true",
    "babel": "rimraf lib && babel src/ -d lib/",
    "copy-assets": "rimraf lib/assets/ && cp -r src/assets/ lib/assets/",
    "copy-styles": "rimraf lib/styles/ && cp -r src/styles/ lib/styles/",
    "build": "npm run babel && npm run copy-assets && npm run copy-styles",
    "prepublish": "npm run build",
    "postinstall": "node -e \"require('fs').stat('lib', function(e,s){process.exit(e || !s.isDirectory() ? 1 : 0)})\" || npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/formidable-landers.git"
  },
  "author": "david.davidson@formidable.com, paula.lavalle@formidable.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/formidable-landers/issues"
  },
  "homepage": "https://github.com/FormidableLabs/formidable-landers#readme",
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-webpack": "^6.4.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "lodash": "^4.15.0",
    "react": "^15.1.0",
    "rimraf": "^2.4.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.9.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-react": "^6.6.0",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.0",
    "mocha": "^3.1.2",
    "normalize.css": "^5.0.0",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.2.0",
    "postcss-inline-svg": "^2.2.0",
    "postcss-loader": "^1.1.1",
    "postcss-url": "^5.1.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.14.1"
  }
}
